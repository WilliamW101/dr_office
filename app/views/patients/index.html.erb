<h1 class='center'> Patients </h1>
<br />

<ul>
  <% @patients.each do |patient| %>
    <li>
      <%= link_to patient.name, patient_path(patient) %> |
      <%= link_to 'Edit', edit_patient_path(patient) %>.
      <%= link_to 'Delete', patient_path(patient), method: :delete, data: { confirm: 'Are You Sure?' } %>.
    </li>
  <% end %>
</ul>
<br />

<div class='center'>
  <%= link_to 'New Patient', new_patient_path, class: 'btn grey lighten-2 black-text' %>
</div>
